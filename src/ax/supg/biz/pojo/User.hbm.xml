<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- 部门映射 -->
<hibernate-mapping package="ax.supg.biz.pojo">

	<class name="User" table="t_user">
		<id name="id" type="java.lang.Long">
			<generator class="native"></generator>
		</id>
		<property name="name" length="30"></property>
		
		<property name="sex" length="6"></property>
		<property name="password" length="20"></property>
		<property name="trueName" length="10"></property>
		
	    <property name="iptCard" length="40"></property>
		
		<property name="tel" length="20"></property>
		<property name="email" length="20"></property>	
			
		<property name="user_info" length="200"></property>
		<property name="scores" length="10"></property>
		
		<property name="file_path" length="100"></property>
		<!-- 关联映射 (t_employee表的外键字段，dept_id) -->
		
	</class>

</hibernate-mapping>



